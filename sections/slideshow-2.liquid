    <style>
        
        .slideshow-wrapper {
          position: relative;
        }
        
        .slideshow {
          position: unset;
          overflow: hidden;
          margin-bottom: 0;
          height: 100vh;
          -webkit-transition: height .6s cubic-bezier(.44,.13,.48,.87);
          -o-transition: height .6s cubic-bezier(.44,.13,.48,.87);
          transition: height .6s cubic-bezier(.44,.13,.48,.87);
        }
        
        @media only screen and (min-width:1024px) {
          .slideshow {
            position: relative;
          }
        }
        
        .slideshow .slick-list,.slideshow .slick-track,.slideshow .slideshow__slide {
          height: 100%;
        }
        
        .slideshow .slick-dots {
          bottom: 30px;
          z-index: 1;
        }
    @media only screen and (max-width:1023px) {
      .slideshow .slick-dots {
        bottom: 10px;
      }
    
      .subtitle_desktop{
        display: none!important;
      }
    }
        
        .slideshow--small {
          height: 65vh;
        }
        
        @media only screen and (min-width:1024px) {
          .slideshow--small {
            height: 475px;
          }
        }
        
        .slideshow--medium {
          height: 270px;
        }
        
        @media only screen and (min-width:1024px) {
          .slideshow--medium {
            height: 74vh;
          }
        }
        
        .slideshow--large {
          height: 375px;
        }
        
        @media only screen and (min-width:1024px) {
          .slideshow--large {
            height: 100vh;
          }
        }
        
        .slideshow__slide {
          position: relative;
          overflow: hidden;
        }
        
        .slideshow__link {
          display: block;
          position: absolute;
          top: 0;
          left: 0;
          right: 0;
          bottom: 0;
        }
        
        .slideshow__link:active,.slideshow__link:focus {
          opacity: 1;
        }
        
        @media only screen and (min-width:1024px) {
          .slideshow__overlay::before {
            content: '';
            position: absolute;
            top: 0;
            right: 0;
            bottom: 0;
            left: 0;
            background-color: #3d4246;
            opacity: .4;
            z-index: 2;
          }
        }
        
        .slideshow__image {
          -webkit-transition: opacity .8s cubic-bezier(.44,.13,.48,.87);
          -o-transition: opacity .8s cubic-bezier(.44,.13,.48,.87);
          transition: opacity .8s cubic-bezier(.44,.13,.48,.87);
          position: absolute;
          top: 0;
          left: 0;
          opacity: 0;
          height: 100%;
          width: 100%;
          background-repeat: no-repeat;
          background-size: cover;
          background-position: center center;
          background-color: transparent;
          z-index: 1;
        }
        @media (max-width: 991px){
          .slideshow__image {
            width: 100%;
          }
        }
    
        @media(max-width: 1023px){
          .slideshow__title{
            color: #2d3033!important;
          }
    
          .slideshow__subtitle{
            color: #373d41!important;
          }
    
          .slideshow__slide.rightimage .slideshow__image {
            background-position: center center!important;
          }
    
          .slideshow__text-wrap .container{
            max-width: initial;
          }
        }
        
        .no-js .slideshow__image,.slick-initialized .slideshow__image {
          opacity: 1;
        }
    
        #insta-feed, .instafeed-shopify{
          margin: 0;
          padding: 80px 0px 160px;
        }
    
        @media(max-width: 768px){
          #insta-feed, .instafeed-shopify{
            display: grid!important;
            grid-template-columns: 50% 50%;
            grid-template-rows: 25% 25% 25% 25%;
            padding: 0px 0px 80px;
          }
          .instafeed-container{
            width: 100%!important;
            padding-top: 100%!important;
          }
        }
        
        .slideshow__text-wrap {
          height: 100%;
          position: relative;
          width: 100%;
        }
        @media (max-width: 991px){
          .slideshow__text-wrap{
            width: 100%;
          }
        }
    
        .slideshow__slide.leftimage .slideshow__image {
          left:0;
        }
        .slideshow__slide.leftimage .slideshow__text-wrap{
          float: right;
        }
        .slideshow__slide.rightimage .slideshow__image {
          right:0;
          left: auto;
        }
        .slideshow__slide.rightimage .slideshow__text-wrap{
          float: left;
        }
        .slideshow__link .slideshow__text-wrap {
          cursor: inherit;
        }
        
        .slideshow__text-wrap--mobile {
          display: none;
        }
    
        
        @media only screen and (min-width:1024px) {
          .slideshow__text-content {
            -webkit-transition: .6s cubic-bezier(.44,.13,.48,.87);
            -o-transition: .6s cubic-bezier(.44,.13,.48,.87);
            transition: .6s cubic-bezier(.44,.13,.48,.87);
            -webkit-transition-delay: .3s;
            -o-transition-delay: .3s;
            transition-delay: .3s;
          }
        }
        
    .slideshow__text-wrap--desktop .slideshow__text-content {
      position: absolute;
      width: 100%;
      top: 50%;
      opacity: 1;
      z-index: 2;
      padding: 0 20px;
       -ms-transform: translateY(-50%);
      -webkit-transform: translateY(-50%);
      transform: translateY(-50%);
